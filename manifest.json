{
    "manifest_version": 3,
    "name": "Auto Summarizer",
    "version": "1.0",
    "description": "Summarize any web page instantly using AI.",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  
